<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.control-panel {
			width: 600px;
		}
	</ui:style>
	<ui:with type="gov.ca.bdo.modeling.dsm2.map.client.images.IconImages"
		field="images"></ui:with>
	<ui:with type="gov.ca.modeling.maps.elevation.client.images.IconImages"
		field="el_images"></ui:with>
	<g:VerticalPanel styleName="{style.control-panel}">
		<g:CaptionPanel captionText="Edit">
			<g:HorizontalPanel>
				<g:ToggleButton ui:field="saveEditButton">
					<g:upFace>Edit</g:upFace>
					<g:downFace>Save</g:downFace>
				</g:ToggleButton>
				<g:CaptionPanel ui:field="elementEditPanel" captionText="Add/Delete Elements">
					<g:HorizontalPanel>
						<g:VerticalPanel>
							<g:ToggleButton ui:field="addButton">
								Add
							</g:ToggleButton>
							<g:ToggleButton ui:field="deleteButton">
								Delete
							</g:ToggleButton>
						</g:VerticalPanel>
						<g:ListBox ui:field="elementTypeBox">
						</g:ListBox>
					</g:HorizontalPanel>
				</g:CaptionPanel>
			</g:HorizontalPanel>
		</g:CaptionPanel>
	</g:VerticalPanel>
</ui:UiBinder> 