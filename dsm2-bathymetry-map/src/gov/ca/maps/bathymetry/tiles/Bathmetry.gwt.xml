<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='bathymap'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name='com.google.gwt.visualization.Visualization' />

	<!-- Other module inherits                                      -->
	<inherits name="com.google.gwt.maps.GoogleMaps" />
	<inherits name='gov.ca.modeling.maps.widgets.ExpandContractMapControl' />
	<inherits name='com.google.gwt.widgetideas.WidgetIdeas'/>
	<!-- Specify the app entry point class.                         -->
	<entry-point
		class='gov.ca.maps.bathymetry.tiles.client.MainEntryPoint' />

	<!-- Specify the paths for translatable code                    -->
	<source path='client' />
	<!-- Host based key dsm2bathymetry.appspot.com -->
	<script
		src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAA6i9cBPasVT8NdiGAvjA1cxSzkjkRSpHaf2PSSRwplNA9Zw-7HBRJ6xC86Pz-l9XLONjPreqHmnuBhg" />
	<!--
		Local host based key
	<script
		src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAAglaP4sjC2PIcowNPLkkkHhSyFs1CXjUHtBZmkd3Zups9CFJIQRRmE076ya1TC23WQEYSofD-wVSCvQ"
		/> -->
</module>
